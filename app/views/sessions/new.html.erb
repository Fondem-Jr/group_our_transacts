<div>
<% if flash[:alert] %>
<p id = "notice"><%= flash[:alert] %> </p>
<% end %>
<%= form_tag do %>
<fieldset>
    <legend>Please Log In </legend>
    <div>
    <%= label_tag :name, 'Name' %>
    <%= text_field_tag :name, params[:name] %>
    </div><br>
    <div>
    <%= submit_tag "Login" %>
    </div>
</fieldset>
<% end %>
</div>